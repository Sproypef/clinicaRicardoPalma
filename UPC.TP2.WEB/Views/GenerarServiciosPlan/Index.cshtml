@{
    ViewBag.Title = "Index";
}

<div class="row">
    <div class="col-md-8">
            <div class="header">
                <h4 class="title">Generar Servicios de Planes de Salud</h4>
            </div>
            <div class="content">
                @{Html.BeginForm("Generar", "GenerarServiciosPlan", FormMethod.Post, new { @class = "some_class", @id= "form_generar_servicio_salud" });}
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label>Seleccionar fecha: Desde</label>
                                <input id="fecha_inicio" name="fecha_inicio" type="text" class="form-control datepicker" placeholder="Fecha inicio" value="01/01/2016">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label>Hasta</label>
                                <input id="fecha_fin" name="fecha_fin" type="text" class="form-control datepicker" placeholder="Fecha fin" value="30/06/2017">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <input id="submit_generar_servicio_salud" name="submit_generar_servicio_salud" value="Generar Servicio de Salud" type="submit" class="btn btn-info btn-fill pull-left" />
                            </div>
                        </div>
                    </div>
                @{Html.EndForm();}

                <br />

                @ViewBag.Message

                <div class="clearfix"></div>
               
            </div>
    </div>

</div>